{
  "name": "angular-boilerplate-server",
  "private": true,
  "engineStrict": true,
  "engines": {
    "node": "20.16.0"
  },
  "scripts": {
    "start": "nodemon",
    "test": "jest",
    "watch": "jest --watch"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/**/*.spec.ts"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "<rootDir>/**/*",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/coverage/**"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@types/express": "^4.17.21",
    "body-parser": "^1.20.1",
    "compression": "^1.7.4",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "express-pino-logger": "^4.0.0",
    "morgan": "^1.10.0",
    "mysql2": "^3.1.2",
    "pino-colada": "^2.2.2",
    "sqlstring": "^2.3.3",
    "string-similarity": "^4.0.4",
    "ts-node": "^10.9.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.2",
    "@types/express-pino-logger": "^4.0.5",
    "@types/jest": "^29.5.12",
    "@types/sinon": "^17.0.3",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "node-env-run": "^4.0.2",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "onchange": "^7.1.0",
    "sinon": "^17.0.1",
    "supertest": "^6.3.4",
    "typescript": "^5.4.3",
    "ts-jest": "^29.1.2"
  }
}
