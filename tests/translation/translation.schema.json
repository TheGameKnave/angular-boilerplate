{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Translation File Schema",
  "type": "object",
  "properties": {
    "Hello world": {
      "type": "string",
      "description": "Main page greeting"
    },
    "Angular Boilerplate": {
      "type": "string",
      "description": "Main page title"
    },
    "v.": {
      "type": "string",
      "description": "Main page version"
    },
    "There are {{count}} components.": {
      "type": "string",
      "description": "Message about the number of components, using a pluralization format"
    }
  },
  "required": [
    "Hello world",
    "Angular Boilerplate",
    "v.",
    "There are {{count}} components."
  ],
  "additionalProperties": false
}
